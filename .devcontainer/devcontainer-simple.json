{
    "name": "Ansible dev-container",
    "image": "ansible-devenv:v1",
    "workspaceFolder": "/home/ansible/project",
    "workspaceMount": "source=/home/jumedina/repos/,target=/home/ansible/project,type=bind,consistency=cached",
    "runArgs": ["--init"],
    "customizations": {
        "vscode": {
            "extensions": [
                "redhat.ansible",
                "redhat.vscode-yaml",
                "ms-python.python",
                "ms-python.debugpy",
                "ms-python.vscode-pylance",
                "ms-python.vscode-python-envs",
                "ms-azuretools.vscode-containers",
                "ms-vscode-remote.remote-containers",
                "ms-vscode-remote.remote-ssh",
                "ms-vscode-remote.remote-ssh-edit",
                "ms-vscode-remote.remote-explorer",
                "alefragnani.project-manager",
                "eamodio.gitlens",
                "esbenp.prettier-vscode",
                "github.vscode-github-actions",
                "gruntfuggly.todo-tree",
                "henriiik.docker-linter",
                "ipierre1.ansible-vault-vscode",
                "mhutchie.git-graph",
                "samuelcolvin.jinjahtml",
                "trunk.io",
                "wholroyd.jinja",
                "vscodevim.vim"
            ]
        }
    }
}
